FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 12;

method          hierarchical;

hierarchicalCoeffs

{
      n               (12 1 1);
      delta           0.001;
      order           xyz;
}

distributed     yes;

roots           ( );


